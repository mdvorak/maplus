<?xml version="1.0" encoding="utf-8" ?>
<webExtender>
  <urls>
    <!-- Example
    <alias name="chrome">chrome://TODO_EXTENSION_ID</alias>
    <alias name="foo">http://www.foo.com</alias>
    -->
  </urls>
  <stylesheets>
    <!-- Example
    <style url="{foo}/*" src="{chrome}/html/foo.css" />
    -->
  </stylesheets>
  <extenders>
    <!-- Example: Files must be placed in same directory as this definition.
    <script url="{foo}/*" name="menu.js" type="text/javascript" />
    <script url="{foo}/mail.html" name="mail.js" type="text/javascript" />
    -->
  </extenders>
</webExtender>