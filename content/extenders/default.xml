<?xml version="1.0" encoding="utf-8" ?>
<webExtender>
  <urls>
    <alias name="chrome">chrome://maplus/content</alias>
    <alias name="melior">http://meliorannis.idnes.cz</alias>
  </urls>
  <extenders>
    <script url="#{melior}/*" name="interop.js" />

    <!-- Generic -->
    <script url="#{melior}/*" name="tooltip.js" />
    <script url="#{melior}/*" name="lib.js" />
    
    <style url="#{melior}/*" src="#{chrome}/html/maplus.css" />
    <script url="#{melior}/*" name="analyzes.js" />
    <script url="#{melior}/*" name="config.js" />
    <script url="#{melior}/*" name="maplus.js" />
    
    <script url="#{melior}/*" name="menu.js" />
    
    <!-- Specific -->
    <script url="#{melior}/obchod.html" name="bestiar.js" />
    <script url="#{melior}/magie.html" name="magie.js" />

    <!-- Post processing -->
    <script url="#{melior}/*" name="postprocessing.js" />
    <script url="#{melior}/*" name="theme.js" />
  </extenders>
</webExtender>
